# Telegram Bot Configuration
# Copy this file to 'settings.yaml' and fill in your actual values

# Necessary values
# Create the bot and obtain the token: https://core.telegram.org/bots/tutorial#obtain-your-bot-token
BOT_TOKEN: "YOUR_BOT_TOKEN"
# Obtain API ID and Hash: https://core.telegram.org/api/obtaining_api_id#obtaining-api-id
API_ID: YOUR_API_ID
API_HASH: "YOU_API_HASH"
BOT_USERNAME: "YOUR_BOT_USERNAME" # Bot username (without the "@")
ADMIN_IDS: [123456789, 987654321] # Telegram user IDs for bot administrator
FEEDBACK_CHAT_ID: -1234567890 # Chat ID where feedback is sent (use /chatid to get chat IDs)

# Optional settings
DEBUG: false
ENABLE_GEMINI_COMMAND: false
ENABLE_IMAGINE_COMMAND: false
ENABLE_MEME_COMMAND: false
ENABLE_TRIVIA_EVENTS: false

# Fill each of these if you enabled their respective settings above
GEMINI_API_KEY: "YOUR_GEMINI_API_KEY" # Get from: https://makersuite.google.com/app/apikey
GEMINI_MODEL: "YOUR_GEMINI_MODEL" # Example: "gemini-1.5-flash"
HUGGINGFACE_TOKEN: "YOUR_HUGGINGFACE_TOKEN" # Get from: https://huggingface.co/settings/tokens
REDDIT_CLIENT_ID: "YOUR_REDDIT_CLIENT_ID" # From: https://www.reddit.com/prefs/apps
REDDIT_CLIENT_SECRET: "YOUR_REDDIT_CLIENT_SECRET"
REDDIT_USER_AGENT: "YOUR_REDDIT_USER_AGENT"